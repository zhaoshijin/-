 page{
    background: #f0f0f2;
    font-size: 14px;
}
.container{
  background:#f0f0f2;
  position:relative;
}

image{
  width:96%;
  position:absolute;
  top:5px;
  bottom:0;
  left:0;
  right:0;
  margin:0 auto;
  height:90px;
}
.details{
  width:96%;
  position:absolute;
  top:110px;
  bottom:0;
  left:0;
  right:0;
  margin:0 auto;
  background:#fafafa;
  border-radius: 5px;
  height:217px;
}
.details text{
  display: block;
  color:#566c94;
  text-align: center;
  line-height: 30px;
}
.details .specil{
  color:#888888;
}
.details .membercard::after{
  content:">";
  color:#888888;
  margin:0 0 0 5px;
} 

/*表单内容*/  
.login-from {  
  margin-top: 20px;  
  flex: auto;    
}  
  
.inputView {  
  background-color: white;  
  line-height: 44px;  
}  
/*输入框*/  
 
  
.loginLab {  
  margin: 15px 15px 15px 10px;   
  font-size: 14px  
}  
.inputText {  
  flex: block;  
  float: right;  
  text-align: right;  
  margin-right: 22px;  
  margin-top: 11px;  
  font-size: 14px  
}  
  
.line {  
  width: 100%;  
  height: 1px;  
  background-color: #cccccc;  
  margin-top: 1px;  
}  
/*按钮*/  
.loginBtnView {  
  width: 100%;  
  height: auto;  
  background-color: #f2f2f2;  
  margin-top: 0px;  
  margin-bottom: 0px;  
  padding-bottom: 0px;  
}  
  
.loginBtn {  
  width: 80%;  
  margin-top: 35px;  
}  
.phonenumber{
  border-bottom-left-radius:5px;
  border-bottom-right-radius:5px;
}
.required{
  color:#888888;
}
.declaration{
  color:#7d9cda;
}
.form-group{
  margin:20px 0 0 0;
  text-align: center;
}
checkbox{
  zoom:70%;
}



