page {
  width: 100%;
  height: 100%;
}
.jifen_con {
  width: 100%;
  height: 100%;
  background: #eff0f2;
}
.jifen_top {
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
  background: #ffffff;
}
.jifen_bottom {
  margin-top: 20rpx;
  background: #ffffff;
}
.jifen_detail {
  width: 100%;
  font-size: 28rpx;
  border-bottom: 1rpx solid #eff0f2;
  padding-left: 3%;
  line-height: 80rpx;
} 
.jifen_can {
  width: 100%;
  font-size: 30rpx;
  border-bottom: 1rpx solid #eff0f2;
  padding-left: 30rpx;
  background: #686a81;
  height: 100rpx;
  line-height: 100rpx;
}
.jifen_shangcheng {
  width: 100%;
  color: red;
  background: #686a81;
  margin-left: 30rpx;
  font-size: 56rpx;
}
.table {
  width: 90%; 
  margin-left: 5%;
  padding: 30rpx 0;
}
.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th,.td {
  padding: 10px;
  text-align: center;
  width: 100%;
}
.th {
  background: #686a81;
  color: white;
  font-size: 30rpx;
  border-right: 1rpx solid #ffffff;
}
.td {
  font-size: 28rpx;
 
  border-right: 1rpx solid #ffffff;
}
.bg_g {
   background: #eff0f2;
}