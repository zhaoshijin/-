.welfare {
  width: 100%;
}
.welfare_top {
  width: 90%;
  padding: 40rpx;
}
.welfare_top image {
  width: 100rpx;
  height: 100rpx;
  float: left;
  padding-right: 20rpx;
}
.welfare_top view {
  font-size: 35rpx;
  color: #0c0936;
}
.welfare_btn {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: white;
  font-size: 30rpx;
  background: #0c0938;
}
.welfare_title {
  border-bottom: 1px solid;
}
.table {
  width: 100%; 
}
.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th,.td {
  padding: 10px;
  text-align: center;
  width: 100%;
}
.th {
  font-size: 30rpx;
  font-weight: bold;
}
.td {
  font-size: 28rpx;
  /* color: #999999;  */
}
