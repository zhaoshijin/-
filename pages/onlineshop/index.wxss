
.container {
    background-color: #F2f2f2;
}
.banner{
    width: 100%;
}
.banner image{
     width: 100%;
     height:200px;
     display: inline-block;
     overflow: hidden;
}
.regbg{
  padding:0.5em 0;
  background:#fff;
  display: flex;
}
.regbg view{
  flex:1;
  font-size:14px;
  text-align: center;
}
.regbg view image{
   width:20px;
   height:20px;
}
.regbg view text{
  display: block;
  color:#888888;
}



.goodslist{  
    background: #fff;  
    display: flex;  
    flex-direction: row;
    flex-wrap:  wrap  ;
    margin: 10px 10px;
}  
.goods{  
    display: flex; 
    flex-direction: row;  
    box-shadow: 0 0 5px #888;  
    margin: 5px 5px;
    width:46.6%;
}  
.good-img{
    position: relative;
    display: flex;
    flex-direction: column;
}
.good-img image{  
    width: 80px;  
    height:90px;  
}  
.good-cont{  
    display: flex;  
    flex: 1;  
    flex-direction: column;  
    font-size: 14px;  
}  
.goods-navigator{  
    display: flex;  
    flex: 1;  
    flex-direction: column;  
    justify-content: center;  
}  
.good-name{  
    display: flex;  
    flex: 1;  
    flex-direction: column; 
    font-weight: 600;    
    line-height:40px;
    text-indent: 5px;
}  
.good-price{  
    display: flex;  
    flex: auto;  
    line-height:40px;
}  
.cart{  
    width: 40px;  
    height: 40px;  
    padding-right: 10px;  
}  