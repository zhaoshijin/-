.container {
    background-color: #F2f2f2;
}

.swiper_box{
    width: 100%;
    text-align: center;
    margin:0 0 2px 0 ;
}
.barcode{
  height:50px;
  width:280px;
  margin:2px 0 2px 0 ;
}
.cardnumber{
  color:#333333;
  font-size: 14px;
}
.slide-image {
    width:300px;
    overflow: hidden;
    height:170px;
}
.regbg{
  padding:0.5em 0;
  text-align: center;
  background:#686a82;
  display:flex;
  font-size:14px;
  color:white;
  height:30px;
  line-height: 30px;
}
.regbg view{
  flex:1;
}
.wrapbottom{
  margin:8px 0 0 0;
}
.mainbottom{
  width:100%;
  height:100%;
  display: flex;
}
.mainbottom view{
  flex:1;
  text-align: center;
  border:1px solid #e3e3e6;
  padding:1em 0;
  font-size:0.7em;
}
.mainbottom view text{
  display:block;
}
.mainbottom view image{
  width:25px;
  height:25px;
}
.graytext{
  color:#9d9d9d;
  line-height: 2em;
}